<h1>Reply</h1>

<%= form_for(@reply) do |f| %>
    <%= render 'shared/error_messages', :object => f.object %>
        <div class="field">
      <%= f.label :user_id %>
      <br/>
      <%= f.text_field :user_id %>
    </div>
    <div class="field">
      <%= f.label :content %>
      <br/>
      <%= f.text_field :content %>
    </div>
    <div class="actions">
      <%= f.submit "Update" %>
    </div>
<% end %>

